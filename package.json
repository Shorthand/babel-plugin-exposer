{
  "version": "0.0.0",
  "description": "Expose modules to global scope for monkey-patch-ability",
  "repository": "Shorthand/babel-plugin-exposer",
  "author": "Andrew Cobby <cobby@shorthand.com>",
  "main": "lib/index.js",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "mocha": "^2.2.5"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "babel src -d lib",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "npm run test -- --watch",
    "prepublish": "npm run clean && npm run build",
    "dev": "npm run build && babel _fixtures/index.js --out-file fixture.js --plugins=lib/index.js"
  },
  "keywords": [
    "export",
    "expose",
    "globals",
    "global",
    "monkey",
    "patch",
    "babel-plugin"
  ]
}
